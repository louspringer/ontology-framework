@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix meta: <http://louspringer.com/ontology/llm-guidance#> .
@prefix metameta: <http://louspringer.com/ontology/metameta#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

meta:LLMGuidanceOntology rdf:type owl:Ontology ;
    dc:title "LLM Guidance Meta-Ontology" ;
    dc:version "1.1" ;
    rdfs:comment "Updated to include guidance for interdisciplinary collaboration." ;
    owl:imports <http://louspringer.com/ontology/metameta> .

# Core Classes
meta:Interpretation rdf:type owl:Class ;
    rdfs:label "Interpretation" ;
    rdfs:comment "Guidance on how to interpret an ontology element" ;
    rdfs:subClassOf metameta:AbstractionDimension .

meta:Action rdf:type owl:Class ;
    rdfs:label "Action" ;
    rdfs:comment "An action that can be taken based on interpretation" .

meta:DomainAnalogy rdf:type owl:Class ;
    rdfs:label "Domain Analogy" ;
    rdfs:subClassOf meta:Interpretation .

# Core Properties
meta:hasAction rdf:type owl:ObjectProperty ;
    rdfs:domain meta:Interpretation ;
    rdfs:range meta:Action .

meta:analogySource rdf:type owl:DatatypeProperty ;
    rdfs:domain meta:DomainAnalogy ;
    rdfs:range xsd:string .

meta:analogyTarget rdf:type owl:DatatypeProperty ;
    rdfs:domain meta:DomainAnalogy ;
    rdfs:range xsd:string .

meta:complexity rdf:type owl:DatatypeProperty ;
    rdfs:domain meta:Interpretation ;
    rdfs:range xsd:integer .

# New Properties
meta:sourceContext rdf:type owl:DatatypeProperty ;
    rdfs:domain meta:Interpretation ;
    rdfs:range xsd:string ;
    rdfs:comment "Context from which the interpretation is derived." .

meta:targetContext rdf:type owl:DatatypeProperty ;
    rdfs:domain meta:Interpretation ;
    rdfs:range xsd:string ;
    rdfs:comment "Context to which the interpretation is applied." .

# New Instances
meta:InterdisciplinaryCollaboration rdf:type meta:Interpretation ;
    rdfs:label "Interdisciplinary Collaboration" ;
    meta:sourceContext "Molecular Biology" ;
    meta:targetContext "Computer Programming" ;
    rdfs:comment "Guidance for facilitating collaboration between biologists and programmers." .
