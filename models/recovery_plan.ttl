# Recovery Plan Model
recovery:NamespaceRecoveryPlan a owl:Class ;
    rdfs:label "Namespace Recovery Plan" ;
    rdfs:comment "Plan to recover from incorrect namespace usage" .

# Current State
recovery:CurrentState a owl:Class ;
    rdfs:label "Current State" ;
    rdfs:comment "Current state of namespace usage" .

recovery:hasExampleOrgUsage a owl:ObjectProperty ;
    rdfs:domain recovery:CurrentState ;
    rdfs:range xsd:boolean ;
    rdfs:label "Has example.org usage" ;
    rdfs:comment "Indicates if example.org is being used" .

recovery:hasRelativePathUsage a owl:ObjectProperty ;
    rdfs:domain recovery:CurrentState ;
    rdfs:range xsd:boolean ;
    rdfs:label "Has relative path usage" ;
    rdfs:comment "Indicates if relative paths are being used" .

# Recovery Steps
recovery:RecoveryStep a owl:Class ;
    rdfs:label "Recovery Step" ;
    rdfs:comment "A step in the recovery process" .

recovery:hasStepOrder a owl:DatatypeProperty ;
    rdfs:domain recovery:RecoveryStep ;
    rdfs:range xsd:integer ;
    rdfs:label "Step order" ;
    rdfs:comment "Order of the recovery step" .

recovery:hasStepDescription a owl:DatatypeProperty ;
    rdfs:domain recovery:RecoveryStep ;
    rdfs:range xsd:string ;
    rdfs:label "Step description" ;
    rdfs:comment "Description of the recovery step" .

# Recovery Steps Instances
recovery:Step1 a recovery:RecoveryStep ;
    recovery:hasStepOrder 1 ;
    recovery:hasStepDescription "Identify all files using example.org" .

recovery:Step2 a recovery:RecoveryStep ;
    recovery:hasStepOrder 2 ;
    recovery:hasStepDescription "Restore original relative paths" .

recovery:Step3 a recovery:RecoveryStep ;
    recovery:hasStepOrder 3 ;
    recovery:hasStepDescription "Validate guidance.ttl loading" .

recovery:Step4 a recovery:RecoveryStep ;
    recovery:hasStepOrder 4 ;
    recovery:hasStepDescription "Implement namespace validation" .

# Current State Instance
recovery:CurrentStateInstance a recovery:CurrentState ;
    recovery:hasExampleOrgUsage true ;
    recovery:hasRelativePathUsage false .

