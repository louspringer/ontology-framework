@prefix rdf: <http:/www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http:/www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http:/www.w3.org/2002/07/owl#> .
@prefix xsd: <http:/www.w3.org/2001/XMLSchema#> .
@prefix recovery: <./models/recovery#> .
@prefix change: <./models/change#> .

# Change Tracking Model
change:ChangeTracking a owl:Class ;
    rdfs:label "Change Tracking" ;
    rdfs:comment "Tracks changes to the recovery plan model" .

change:hasChangeID a owl:DatatypeProperty ;
    rdfs:domain change:ChangeTracking ;
    rdfs:range xsd:string ;
    rdfs:label "Change ID" .

change:hasChangeDate a owl:DatatypeProperty ;
    rdfs:domain change:ChangeTracking ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Change date" .

change:hasChangeDescription a owl:DatatypeProperty ;
    rdfs:domain change:ChangeTracking ;
    rdfs:range xsd:string ;
    rdfs:label "Change description" .

change:hasChangeType a owl:DatatypeProperty ;
    rdfs:domain change:ChangeTracking ;
    rdfs:range xsd:string ;
    rdfs:label "Change type" .

change:hasAffectedComponent a owl:ObjectProperty ;
    rdfs:domain change:ChangeTracking ;
    rdfs:range owl:Thing ;
    rdfs:label "Affected component" .

