# Physical Layout Model
ex:ComponentLayout a owl:Class ;
    rdfs:label "Component Layout" ;
    rdfs:comment "Defines physical arrangement of components on mounting board." .

ex:Position a owl:Class ;
    rdfs:label "Position" ;
    rdfs:comment "Defines a specific location on the mounting board." .

ex:hasPosition a owl:ObjectProperty ;
    rdfs:domain ex:ComponentLayout ;
    rdfs:range ex:Position .

ex:adjacentTo a owl:ObjectProperty ;
    rdfs:domain ex:Position ;
    rdfs:range ex:Position ;
    rdfs:comment "Indicates physical adjacency between components." .

# Layout Components
ex:CrossoverBoardLayout a ex:ComponentLayout ;
    rdfs:label "Crossover Board Physical Layout" ;
    ex:parentSolution ex:MidTierImplementation ;
    ex:dimensions """
        Width: 8 inches
        Height: 6 inches
        Depth: 1.5 inches"""^^xsd:string ;
    ex:mountingDetails """
        Material: FR-4 terminal board
        Mounting: 4x corner holes
        Clearance: 0.25 inch minimum"""^^xsd:string .

# Component Positions
ex:InputPosition a ex:Position ;
