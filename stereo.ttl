ex:CrossoverSafetySpec a ex:SafetyRequirements ;
    ex:terminalSpacing "0.25 inch minimum"^^xsd:string ;
    ex:strainRelief "Required on all external connections"^^xsd:string ;
    ex:ventilation "Holes near power resistors"^^xsd:string ;
    ex:grounding "Central ground point with star topology"^^xsd:string .

# Layout Verification
ex:LayoutVerification a owl:Class ;
    rdfs:label "Layout Verification" ;
    rdfs:comment "Defines verification steps for physical layout." .

ex:CrossoverLayoutChecks a ex:LayoutVerification ;
