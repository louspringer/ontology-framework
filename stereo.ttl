# Diagram Specifications
ex:DiagramSpecification a owl:Class ;
    rdfs:label "Diagram Specification" ;
    rdfs:comment "Defines diagram specifications for system visualization." .

ex:CrossoverDiagram a ex:DiagramSpecification ;
    rdfs:label "Crossover Component Layout Diagram" ;
    ex:diagramType "PlantUML" ;
    ex:version "1.0" ;
    ex:parentSolution ex:MidTierImplementation ;
    ex:sourceCode """@startuml
!theme plain
skinparam componentStyle rectangle

component "Input Terminal" as input
component "L-Pad Network" as lpad {
  component "R1 (8Ω)" as r1
  component "R2 (16Ω)" as r2
}

package "High-Pass" {
  component "L1\n0.5mH" as l1
  component "C1\n10µF" as c1
}

package "Low-Pass" {
  component "L2\n1.0mH" as l2
  component "C2\n20µF" as c2
}

package "Zobel" {
  component "Rz\n10Ω" as rz
  component "Cz\n10µF" as cz
}

component "Horn Out" as hornout
component "RS-100 Out" as rs100out

input --> lpad
lpad --> l1
l1 --> c1
c1 --> hornout
lpad --> l2
l2 --> c2
c2 --> rs100out
hornout -- zobel

note right of input : "Top center"
note right of hornout : "Bottom left"
note right of rs100out : "Bottom right"
@enduml"""^^xsd:string ;
    ex:visualizes ex:CrossoverBoardLayout ;
    ex:showsComponents """
        - Input Terminal
        - L-Pad Network (R1, R2)
        - High-Pass Section (L1, C1)
        - Low-Pass Section (L2, C2)
        - Zobel Network (Rz, Cz)
        - Output Terminals"""^^xsd:string .

# Speaker Specifications
ex:RS100Specifications a ex:Constraint ;
    rdfs:label "RS-100 Crossover Specifications" ;
    rdfs:comment "The Klipsch RS-100 has a fixed crossover network." ;
    ex:crossoverType "Fixed passive, internal" ;
    ex:crossoverFrequency "2400"^^xsd:integer ;
    ex:crossoverSlope "12dB/octave"^^xsd:string ;
    ex:speakerImpedance "8 ohms nominal"^^xsd:string .

# New Risk
ex:InternalCrossoverInteraction a ex:Risk ;
    rdfs:label "Internal Crossover Interaction Risk" ;
    rdfs:comment "Risk of phase and frequency response issues from cascading crossovers." ;
    ex:mitigationStrategy """
        Options:
        1. Bypass internal RS-100 crossover (requires speaker modification)
        2. Design external crossover to work with internal crossover characteristics
        3. Use RS-100 internal crossover and only build horn crossover"""^^xsd:string .

# Analysis of Horn-Only Crossover Approach
ex:HornOnlyCrossoverAnalysis a ex:SolutionSpace ;
    rdfs:label "Horn-Only Crossover Analysis" ;
    ex:parentSolution ex:PassiveCrossoverNetwork ;
    ex:advantages """
        1. Simpler implementation
        2. No modification to RS-100
        3. Lower component count
        4. Reduced cost
        5. Maintains RS-100 warranty"""^^xsd:string ;
    ex:challenges """
        1. Phase alignment complexity at crossover point
        2. Limited control over RS-100 response
        3. Potential frequency response gaps or overlaps
        4. More complex level matching
        5. No control over RS-100 crossover characteristics"""^^xsd:string ;
    ex:technicalIssues """
        1. Fixed 2.4kHz crossover in RS-100 may not be optimal for horn
        2. Double-filtering effect in crossover region
        3. Potential comb filtering from phase interactions
        4. Limited ability to adjust crossover slopes
        5. Complex impedance interactions"""^^xsd:string .

# Required Testing Protocol
ex:HornOnlyTestingProtocol a ex:LayoutVerification ;
    rdfs:label "Horn-Only Testing Protocol" ;
    ex:measurements """
        1. Combined frequency response at crossover point
        2. Phase response through crossover region
        3. Impulse response measurements
        4. Group delay measurements
        5. Distortion measurements at crossover frequency"""^^xsd:string ;
    ex:criticalPoints """
        1. 2.2kHz - 2.6kHz region coverage
        2. Phase coherence at listening position
        3. Level matching accuracy
        4. Time alignment verification"""^^xsd:string .

# New Risk
ex:FrequencyResponseGaps a ex:Risk ;
    rdfs:label "Frequency Response Gaps Risk" ;
    rdfs:comment "Risk of uneven frequency response due to mismatched crossover points." ;
    ex:impactAreas """
        1. Vocal clarity (critical 2-3kHz region)
        2. Instrument timbre accuracy
        3. Soundstage coherence
        4. Overall system integration"""^^xsd:string .

# New Solution Space
ex:SubwooferAlternative a ex:SolutionSpace ;
    rdfs:label "Passive Subwoofer Alternative" ;
    ex:parentSolution ex:OfficeAudioIntegration ;
    ex:advantages """
        1. Simpler integration - no crossover issues with RS-100
        2. RS-100 operates in its intended range
        3. Better bass extension
        4. No modification to existing speakers
        5. Cleaner transition between frequency ranges"""^^xsd:string ;
    ex:implementation """
        1. Use RS-100 as full-range/mid-high
        2. Add passive subwoofer for low frequencies
        3. Utilize RX-V4A's built-in crossover
        4. Take advantage of room correction (YPAO)"""^^xsd:string ;
    ex:benefits """
        1. Avoids critical 2-3kHz crossover region
        2. Maintains RS-100's intended performance
        3. Better system integration
        4. Uses receiver's DSP capabilities
        5. More flexible placement options"""^^xsd:string .

# Comparison Analysis
