    ex:coordinates "Top center, 1 inch from edge"^^xsd:string ;
    ex:mountingType "Barrier strip"^^xsd:string .

ex:LPadPosition a ex:Position ;
    rdfs:label "L-Pad Network Position" ;
    ex:coordinates "2 inches from top, left side"^^xsd:string ;
    ex:thermalConsiderations "Vertical mount for cooling"^^xsd:string .

ex:HighPassSection a ex:Position ;
    rdfs:label "High-Pass Section Position" ;
    ex:components """
        L1 (0.5mH inductor)
