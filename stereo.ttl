@prefix ex: <./stereo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes
ex:ProblemSpace a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Problem Space" ;
    rdfs:comment "Defines the overarching challenges or issues in the audio setup." .

ex:SolutionSpace a owl:Class ;
    rdfs:label "Solution Space" ;
    rdfs:comment "Defines potential solutions or strategies to address the problem space." .

ex:Principle a owl:Class ;
    rdfs:label "Principle" ;
    rdfs:comment "Defines guiding principles for decision-making and design." .

ex:Constraint a owl:Class ;
    rdfs:label "Constraint" ;
    rdfs:comment "Defines the limitations or fixed conditions affecting the solution space." .

ex:Risk a owl:Class ;
    rdfs:label "Risk" ;
    rdfs:comment "Defines potential risks or challenges in implementing solutions." .

ex:EvolutionaryConcern a owl:Class ;
    rdfs:label "Evolutionary Concern" ;
    rdfs:comment "Defines considerations for future-proofing or scalability." .

# Individuals (Problem Space)
ex:SoundIsolation a ex:ProblemSpace ;
    rdfs:label "Sound Isolation" ;
    rdfs:comment "Challenge of minimizing sound leakage between the loft and family room." .

ex:WholeAreaSoundReinforcement a ex:ProblemSpace ;
    rdfs:label "Whole-Area Sound Reinforcement" ;
    rdfs:comment "Challenge of achieving cohesive sound across the loft and family room." .

# Individuals (Solution Space)
ex:YamahaRXV4ASetup a ex:SolutionSpace ;
    ex:implementationComplexity "Medium"^^xsd:string ;
    ex:estimatedCost "63"^^xsd:integer ;
    rdfs:label "Yamaha RX-V4A Setup" ;
    rdfs:comment "Proposed solution to replace Fosi BT30D with Yamaha RX-V4A and integrate Hafler DH-220." .

ex:SpeakerPlacementOptimization a ex:SolutionSpace ;
    rdfs:label "Speaker Placement Optimization" ;
    rdfs:comment "Proposed optimization of speaker and subwoofer placement for better sound reinforcement." .

# Individuals (Principles)
ex:IfItWorksDontFixIt a ex:Principle ;
    rdfs:label "If it works, don't fix it" ;
    rdfs:comment "Avoid unnecessary changes to functional components." .

ex:ElectronicCompensationsBeforeAcousticPanels a ex:Principle ;
    rdfs:label "Electronic Compensations Before Acoustic Panels" ;
    rdfs:comment "Prioritize electronic solutions over physical treatments to preserve home aesthetics." .

ex:CostEffectiveImprovements a ex:Principle ;
    rdfs:label "Cost-Effective Improvements" ;
    rdfs:comment "Strive for efficient and affordable upgrades." .

# Individuals (Constraints)
ex:YamahaRXV6AConstraint a ex:Constraint ;
    rdfs:label "Yamaha RX-V6A Constraint" ;
    rdfs:comment "Defines the RX-V6A as a fixed component of the family room setup." .

ex:Bose901Integration a ex:Constraint ;
    rdfs:label "Bose 901 Integration" ;
    rdfs:comment "The Bose 901 speakers must remain integral to the family room setup." .

# Individuals (Risks)
ex:OverComplicationRisk a ex:Risk ;
    rdfs:label "Over-Complication Risk" ;
    rdfs:comment "Risk of over-engineering the solution and adding unnecessary complexity." .

ex:SoundLeakageRisk a ex:Risk ;
    rdfs:label "Sound Leakage Risk" ;
    rdfs:comment "Risk of inadequate sound isolation between family room and loft." .

# Individuals (Evolutionary Concerns)
ex:HaflerDH220UpgradePotential a ex:EvolutionaryConcern ;
    ex:potentialRisks "Collector value could be affected by modifications."^^xsd:string ;
    rdfs:label "Hafler DH-220 Upgrade Potential" ;
    rdfs:comment "Consideration for upgrading Hafler DH-220 amplifiers for improved performance and value." .

ex:FutureCompatibility a ex:EvolutionaryConcern ;
    ex:examples "Ensure HDMI 2.1, eARC, and MusicCast support."^^xsd:string ;
    rdfs:label "Future Compatibility" ;
    rdfs:comment "Ensuring that new equipment integrates seamlessly with the existing system." .

# New Problem Space Individual
ex:OfficeAudioIntegration a ex:ProblemSpace ;
    rdfs:label "Office Audio Integration" ;
    rdfs:comment "Challenge of integrating free-standing horns and RS-100 speakers in the office space." .

# New Solution Space Individuals
ex:ActiveCrossoverSetup a ex:SolutionSpace ;
    ex:implementationComplexity "High"^^xsd:string ;
    ex:estimatedCost "High"^^xsd:string ;
    rdfs:label "Active Crossover Configuration" ;
    rdfs:comment "Implementation using active crossover before amplification stages." .

ex:PassiveCrossoverSetup a ex:SolutionSpace ;
    ex:implementationComplexity "Low"^^xsd:string ;
    ex:estimatedCost "Low"^^xsd:string ;
    rdfs:label "Passive Crossover Configuration" ;
    rdfs:comment "Implementation using passive crossover after amplification." .

# New Solution Space Details
ex:PassiveCrossoverNetwork a ex:SolutionSpace ;
    rdfs:label "Passive Crossover Network Design" ;
    ex:crossoverFrequency "2400"^^xsd:integer ;
    ex:slopeOrder "12"^^xsd:integer ;
    ex:componentType "L-pad with Zobel network"^^xsd:string ;
    rdfs:comment "Specific passive crossover design for horn/RS-100 integration." ;
    ex:addressesProblem ex:OfficeAudioIntegration ;
    ex:guidedByPrinciple ex:CostEffectiveImprovements .

# New Constraint
ex:CrossoverSpecifications a ex:Constraint ;
    rdfs:label "Crossover Design Specifications" ;
    ex:minimumSpec "2nd order slopes"^^xsd:string ;
    ex:impedanceMatching "Required L-pad for horn attenuation"^^xsd:string ;
    rdfs:comment "Technical specifications for the passive crossover implementation." .

# New Risk
ex:SensitivityMismatchRisk a ex:Risk ;
    rdfs:label "Sensitivity Mismatch Risk" ;
    rdfs:comment "Risk of uneven frequency response due to different speaker sensitivities." .

# New Constraints
ex:ExistingHardware a ex:Constraint ;
    rdfs:label "Existing Hardware Integration" ;
    rdfs:comment "Must integrate RX-V4A and Klipsch RS-100 speakers into the solution." .

# New Risk
ex:CrossoverComplexityRisk a ex:Risk ;
    rdfs:label "Crossover Complexity Risk" ;
    rdfs:comment "Risk of system complexity affecting reliability and ease of use." .

# Crossover Implementation Options
ex:BasicPassiveCrossover a ex:SolutionSpace ;
    rdfs:label "Basic Passive Crossover" ;
    ex:implementationComplexity "Low"^^xsd:string ;
    ex:estimatedCost "50-75"^^xsd:integer ;
    ex:qualityRating "3"^^xsd:integer ;
    ex:componentList """
        - Basic 2nd order crossover
        - Standard electrolytic capacitors
        - Iron core inductors
        - Basic L-pad"""^^xsd:string ;
    rdfs:comment "Basic functional implementation with budget components." .

ex:MidTierPassiveCrossover a ex:SolutionSpace ;
    rdfs:label "Mid-Tier Passive Crossover" ;
    ex:implementationComplexity "Medium"^^xsd:string ;
    ex:estimatedCost "175"^^xsd:integer ;
    ex:qualityRating "4"^^xsd:integer ;
    ex:componentList """
        - 2nd order crossover
        - Mixed film/electrolytic capacitors
        - Air core inductors
        - Precision L-pad
        - Basic Zobel network"""^^xsd:string ;
    rdfs:comment "Balance of quality and cost with better components." .

ex:PremiumPassiveCrossover a ex:SolutionSpace ;
    rdfs:label "Premium Passive Crossover" ;
    ex:implementationComplexity "High"^^xsd:string ;
    ex:estimatedCost "350"^^xsd:integer ;
    ex:qualityRating "5"^^xsd:integer ;
    ex:componentList """
        - 3rd order crossover
        - Mundorf/Jantzen capacitors
        - Premium air core inductors
        - Precision L-pad
        - Full Zobel network
        - Notch filters"""^^xsd:string ;
    rdfs:comment "Audiophile-grade implementation with premium components." .

# Mid-Tier Implementation Details
ex:MidTierImplementation a ex:SolutionSpace ;
    rdfs:label "Mid-Tier Crossover Implementation Details" ;
    ex:parentSolution ex:MidTierPassiveCrossover ;
    ex:specifications """
        Crossover Frequency: 2.5kHz
        Slope: 12dB/octave (2nd order)
        Input Impedance: 8 ohms
        Power Handling: 100W RMS"""^^xsd:string ;
    ex:components """
        High-Pass Section (Horn):
        - C1: 10µF Dayton Audio polypropylene (250V)
        - L1: 0.5mH air-core inductor (18AWG)
        - R1: 8Ω L-pad series (25W)
        - R2: 16Ω L-pad parallel (25W)
        
        Low-Pass Section (RS-100):
        - L2: 1.0mH air-core inductor (18AWG)
        - C2: 20µF Dayton Audio polypropylene (250V)
        
        Zobel Network:
        - Rz: 10Ω (10W)
        - Cz: 10µF film capacitor"""^^xsd:string ;
    ex:buildInstructions """
        1. Mount components on high-quality terminal board
        2. Use 16AWG OFC wire for connections
        3. Keep inductors perpendicular orientation
        4. Minimum 2" spacing between inductors
        5. Short signal paths
        6. Heavy gauge binding posts"""^^xsd:string ;
    ex:testingProtocol """
        1. DC resistance check
        2. Frequency response measurement
        3. Phase alignment verification
        4. Thermal stress test (1 hour at 50% power)
        5. Listening evaluation"""^^xsd:string .

# Layout Specifications
ex:CrossoverLayout a ex:SolutionSpace ;
    rdfs:label "Crossover Physical Layout" ;
    ex:parentSolution ex:MidTierImplementation ;
    ex:layoutDimensions "8 inches x 6 inches"^^xsd:string ;
    ex:mountingType "Terminal board with barrier strips"^^xsd:string ;
    ex:componentSpacing "Minimum 2 inches between inductors"^^xsd:string ;
    ex:wireGauge "16AWG OFC for signal path"^^xsd:string .

# Relationships
ex:addressesProblem a owl:ObjectProperty ;
    rdfs:domain ex:SolutionSpace ;
    rdfs:range ex:ProblemSpace ;
    rdfs:label "addresses problem" ;
    rdfs:comment "Links a solution to a problem space." .

ex:guidedByPrinciple a owl:ObjectProperty ;
    rdfs:label "guided by principle" ;
    rdfs:comment "Links a solution or decision to a guiding principle." .

ex:constrainedBy a owl:ObjectProperty ;
    rdfs:label "constrained by" ;
    rdfs:comment "Links a problem or solution to a constraint." .

ex:mitigatesRisk a owl:ObjectProperty ;
    rdfs:label "mitigates risk" ;
    rdfs:comment "Links a solution to a risk it mitigates." .

ex:addressesEvolutionaryConcern a owl:ObjectProperty ;
    rdfs:label "addresses evolutionary concern" ;
    rdfs:comment "Links a solution to an evolutionary concern." .

# Examples of Usage
ex:YamahaRXV4ASetup ex:addressesProblem ex:SoundIsolation, ex:WholeAreaSoundReinforcement ;
    ex:guidedByPrinciple ex:ElectronicCompensationsBeforeAcousticPanels, ex:CostEffectiveImprovements ;
    ex:constrainedBy ex:YamahaRXV6AConstraint ;
    ex:addressesEvolutionaryConcern ex:FutureCompatibility .

ex:SpeakerPlacementOptimization ex:addressesProblem ex:WholeAreaSoundReinforcement ;
    ex:guidedByPrinciple ex:CostEffectiveImprovements ;
    ex:mitigatesRisk ex:SoundLeakageRisk .

# Relationship Instances
ex:ActiveCrossoverSetup 
    ex:addressesProblem ex:OfficeAudioIntegration ;
    ex:guidedByPrinciple ex:CostEffectiveImprovements ;
    ex:constrainedBy ex:ExistingHardware ;
    ex:mitigatesRisk ex:CrossoverComplexityRisk .

ex:PassiveCrossoverSetup 
    ex:addressesProblem ex:OfficeAudioIntegration ;
