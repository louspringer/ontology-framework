# Signal Path Model
ex:SignalPath a owl:Class ;
    rdfs:label "Signal Path" ;
    rdfs:comment "Defines the routing of audio signals through components." .

ex:MainSignalPath a ex:SignalPath ;
    rdfs:label "Main Signal Path" ;
    ex:wireSpec "16AWG OFC"^^xsd:string ;
    ex:routing """
        INPUT → L-pad → High-Pass Branch → HORN OUT
              └→ Low-Pass Branch → RS-100 OUT"""^^xsd:string .

# Physical Relationships
ex:InputPosition 
    ex:adjacentTo ex:LPadPosition ;
    ex:wiringGauge "16AWG"^^xsd:string .

ex:LPadPosition
    ex:adjacentTo ex:HighPassSection ;
    ex:adjacentTo ex:LowPassSection .

ex:HighPassSection
    ex:adjacentTo ex:ZobelPosition ;
    ex:minimumSpacing "2 inches from Low-Pass Section"^^xsd:string .

