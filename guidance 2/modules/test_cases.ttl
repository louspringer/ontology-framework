@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

rdf:type rdfs:label "22-rdf-syntax-ns#type" ;
    rdfs:comment "Description of 22-rdf-syntax-ns#type" ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    owl:versionInfo "1.0.0" .

rdfs:comment rdfs:label "rdf-schema#comment" ;
    rdfs:comment "Description of rdf-schema#comment" ;
    rdfs:domain owl:Thing ;
    rdfs:range rdfs:Literal ;
    owl:versionInfo "1.0.0" .

rdfs:domain rdfs:label "rdf-schema#domain" ;
    rdfs:comment "Description of rdf-schema#domain" ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    owl:versionInfo "1.0.0" .

rdfs:label rdfs:label "rdf-schema#label" ;
    rdfs:comment "Description of rdf-schema#label" ;
    rdfs:domain owl:Thing ;
    rdfs:range rdfs:Literal ;
    owl:versionInfo "1.0.0" .

rdfs:range rdfs:label "rdf-schema#range" ;
    rdfs:comment "Description of rdf-schema#range" ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    owl:versionInfo "1.0.0" .

owl:versionInfo rdfs:label "owl#versionInfo" ;
    rdfs:comment "Description of owl#versionInfo" ;
    rdfs:domain owl:Thing ;
    rdfs:range rdfs:Literal ;
    owl:versionInfo "1.0.0" .

<https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/test#hasValidationRule> a owl:ObjectProperty ;
    rdfs:label "Has Validation Rule" ;
    rdfs:comment "Links a test case to its validation rules" ;
    rdfs:domain <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/test#TestCase> ;
    rdfs:range <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/test#ValidationRule> ;
    owl:versionInfo "1.0.0" .

<https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/test_cases> a owl:Ontology ;
    rdfs:label "Test Cases Ontology" ;
    rdfs:comment "Ontology for defining test case validation rules" ;
    owl:versionInfo "1.0.0" .

<https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/test#TestCase> a owl:Class ;
    rdfs:label "Test Case" ;
    rdfs:comment "A test case for validating ontology components" ;
    owl:versionInfo "1.0.0" .

<https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/test#ValidationRule> a owl:Class ;
    rdfs:label "Validation Rule" ;
    rdfs:comment "A rule for validating ontology components" ;
    owl:versionInfo "1.0.0" .

