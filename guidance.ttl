@prefix guidance: <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdf:type rdfs:label "22-rdf-syntax-ns#type" ;
    rdfs:comment "Description of 22-rdf-syntax-ns#type" ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    owl:versionInfo "1.0.0" .

rdfs:subClassOf rdfs:label "rdf-schema#subClassOf" ;
    rdfs:comment "Description of rdf-schema#subClassOf" ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    owl:versionInfo "1.0.0" .

guidance:ConformanceLevelShape a sh:NodeShape ;
    rdfs:label "ConformanceLevel Shape"@en ;
    rdfs:comment "SHACL shape for validating conformancelevels"@en ;
    sh:property guidance:property_hasComplianceMetrics,
        guidance:property_hasMinimumRequirements,
        guidance:property_hasStringRepresentation,
        guidance:property_hasValidationRules ;
    sh:targetClass guidance:ConformanceLevel .

guidance:ErrorHandling a guidance:TestPhase ;
    rdfs:label "Error Handling"@en ;
    rdfs:comment "Testing error handling scenarios"@en .

guidance:GuidanceOntology a guidance:uri ;
    rdfs:label "Guidance Ontology"@en ;
    rdfs:comment "Core guidance ontology for the ontology framework"@en .

guidance:IntegrationProcessShape a sh:NodeShape ;
    rdfs:label "IntegrationProcess Shape"@en ;
    rdfs:comment "SHACL shape for validating integrationprocesss"@en ;
    sh:property guidance:property_hasIntegrationStep,
        guidance:property_stepDescription,
        guidance:property_stepOrder ;
    sh:targetClass guidance:IntegrationProcess .

guidance:IntegrationStep1 a guidance:IntegrationStep ;
    rdfs:label "Step 1"@en ;
    rdfs:comment "First integration step"@en ;
    guidance:stepDescription "First integration step" ;
    guidance:stepOrder "1" .

guidance:IntegrationStepShape a sh:NodeShape ;
    rdfs:label "IntegrationStep Shape"@en ;
    rdfs:comment "SHACL shape for validating integrationsteps"@en ;
    sh:property guidance:property_stepDescription,
        guidance:property_stepOrder ;
    sh:targetClass guidance:IntegrationStep .

guidance:IntegrationTestPhase a guidance:TestPhase ;
    rdfs:label "IntegrationTestPhase" .

guidance:MODERATE a guidance:ConformanceLevel ;
    rdfs:label "Moderate Conformance"@en ;
    rdfs:comment "Moderate validation rules allowing some flexibility"@en ;
    guidance:hasComplianceMetrics "80% compliance required" ;
    guidance:hasMinimumRequirements "Core properties must be present" ;
    guidance:hasStringRepresentation "MODERATE" ;
    guidance:hasValidationRules "Most validation rules must pass" .

guidance:ModelConformanceShape a sh:NodeShape ;
    rdfs:label "ModelConformance Shape"@en ;
    rdfs:comment "SHACL shape for validating modelconformances"@en ;
    sh:property guidance:property_conformanceLevel ;
    sh:targetClass guidance:ModelConformance .

guidance:OWLValidationPattern a guidance:ValidationPattern ;
    rdfs:label "OWLValidationPattern" .

guidance:PerformanceTesting a guidance:TestPhase ;
    rdfs:label "Performance Testing"@en ;
    rdfs:comment "Testing performance of validation rules"@en .

guidance:Phase1 a guidance:TestPhase ;
    rdfs:label "Phase 1"@en ;
    rdfs:comment "The first phase of testing"@en .

guidance:PropertyValidationPattern a guidance:ValidationPattern ;
    rdfs:label "Property Validation Pattern"@en ;
    rdfs:comment "Pattern for validating SHACL property constraints"@en ;
    guidance:hasValidationPattern "sh:property" ;
    guidance:patternDescription "Validates SHACL property constraints" ;
    guidance:patternPriority "HIGH" ;
    guidance:requiresValidation true .

guidance:RELAXED a guidance:ConformanceLevel ;
    rdfs:label "Relaxed Conformance"@en ;
    rdfs:comment "Relaxed validation rules with significant flexibility"@en ;
    guidance:hasComplianceMetrics "60% compliance required" ;
    guidance:hasMinimumRequirements "Essential properties must be present" ;
    guidance:hasStringRepresentation "RELAXED" ;
    guidance:hasValidationRules "Basic validation rules must pass" .

guidance:SHACLIntegrationRequirement a guidance:IntegrationRequirement ;
    rdfs:label "SHACL Integration Requirement"@en ;
    rdfs:comment "Requirements for SHACL integration"@en .

guidance:SHACLValidation a owl:Class ;
    rdfs:label "SHACL Validation"@en ;
    rdfs:comment "Rules and patterns for SHACL validation"@en .

guidance:SHACLValidationPattern a guidance:ValidationPattern ;
    rdfs:label "SHACLValidationPattern" .

guidance:STRICT a guidance:ConformanceLevel ;
    rdfs:label "Strict Conformance"@en ;
    rdfs:comment "Strict validation rules requiring full compliance"@en ;
    guidance:hasComplianceMetrics "100% compliance required" ;
    guidance:hasMinimumRequirements "All required properties must be present" ;
    guidance:hasStringRepresentation "STRICT" ;
    guidance:hasValidationRules "All validation rules must pass" .

guidance:SampleTestProtocol a guidance:TestProtocol ;
    rdfs:label "Sample Test Protocol"@en ;
    rdfs:comment "A sample test protocol for unit testing"@en ;
    guidance:conformanceLevel "STRICT" ;
    guidance:requiresNamespaceValidation true ;
    guidance:requiresPrefixValidation true .

guidance:ShapeValidationPattern a guidance:ValidationPattern ;
    rdfs:label "Shape Validation Pattern"@en ;
    rdfs:comment "Pattern for validating SHACL shapes"@en ;
    guidance:hasValidationPattern "sh:NodeShape" ;
    guidance:patternDescription "Validates SHACL shape structure" ;
    guidance:patternPriority "HIGH" ;
    guidance:requiresValidation true .

guidance:StandardCoverage a guidance:TestCoverage ;
    rdfs:label "Standard Coverage"@en ;
    rdfs:comment "Standard test coverage requirements"@en .

guidance:TestImprovements a guidance:TODO ;
    rdfs:label "Test Improvements"@en ;
    rdfs:comment "Future improvements for testing"@en .

guidance:TestOrganization a guidance:TODO ;
    rdfs:label "Test Organization"@en ;
    rdfs:comment "Improvements for test organization"@en .

guidance:TestProtocolShape a sh:NodeShape ;
    rdfs:label "TestProtocol Shape"@en ;
    rdfs:comment "SHACL shape for validating testprotocols"@en ;
    sh:property guidance:property_hasTestPhase,
        guidance:property_requiresNamespaceValidation,
        guidance:property_requiresPrefixValidation ;
    sh:targetClass guidance:TestProtocol .

guidance:UnitTestPhase a guidance:TestPhase ;
    rdfs:label "UnitTestPhase" .

guidance:hasValidationPattern a owl:ObjectProperty ;
    rdfs:label "has validation pattern"@en ;
    rdfs:comment "The validation pattern"@en ;
    rdfs:domain guidance:ValidationPattern ;
    rdfs:range xsd:string .

guidance:patternDescription a owl:ObjectProperty ;
    rdfs:label "pattern description"@en ;
    rdfs:comment "Description of the validation pattern"@en ;
    rdfs:domain guidance:ValidationPattern ;
    rdfs:range xsd:string .

guidance:patternPriority a owl:ObjectProperty ;
    rdfs:label "pattern priority"@en ;
    rdfs:comment "Priority of the validation pattern"@en ;
    rdfs:domain guidance:ValidationPattern ;
    rdfs:range xsd:string .

guidance:requiresValidation a owl:ObjectProperty ;
    rdfs:label "requires validation"@en ;
    rdfs:comment "Whether validation is required"@en ;
    rdfs:domain guidance:ValidationPattern ;
    rdfs:range xsd:boolean .

guidance:IntegrationRequirement a owl:Class ;
    rdfs:label "Integration Requirement"@en ;
    rdfs:comment "Requirements for integrating validation components"@en .

guidance:TestCoverage a owl:Class ;
    rdfs:label "Test Coverage"@en ;
    rdfs:comment "Coverage metrics for testing"@en .

guidance:conformanceLevel a owl:ObjectProperty ;
    rdfs:label "conformance level"@en ;
    rdfs:comment "The level of conformance required"@en ;
    rdfs:domain guidance:ModelConformance ;
    rdfs:range xsd:string .

guidance:hasComplianceMetrics a owl:ObjectProperty ;
    rdfs:label "has compliance metrics"@en ;
    rdfs:comment "The compliance metrics for this conformance level"@en ;
    rdfs:domain guidance:ConformanceLevel ;
    rdfs:range xsd:string .

guidance:hasIntegrationStep a owl:ObjectProperty ;
    rdfs:label "has integration step"@en ;
    rdfs:comment "Links a process to its steps"@en ;
    rdfs:domain guidance:IntegrationProcess ;
    rdfs:range guidance:IntegrationStep .

guidance:hasMinimumRequirements a owl:ObjectProperty ;
    rdfs:label "has minimum requirements"@en ;
    rdfs:comment "The minimum requirements for this conformance level"@en ;
    rdfs:domain guidance:ConformanceLevel ;
    rdfs:range xsd:string .

guidance:hasStringRepresentation a owl:ObjectProperty ;
    rdfs:label "has string representation"@en ;
    rdfs:comment "The string representation of a conformance level"@en ;
    rdfs:domain guidance:ConformanceLevel ;
    rdfs:range xsd:string .

guidance:hasTestPhase a owl:ObjectProperty ;
    rdfs:label "has test phase"@en ;
    rdfs:comment "Links a test protocol to its phases"@en ;
    rdfs:domain guidance:TestProtocol ;
    rdfs:range guidance:TestPhase .

guidance:hasValidationRules a owl:ObjectProperty ;
    rdfs:label "has validation rules"@en ;
    rdfs:comment "The validation rules for this conformance level"@en ;
    rdfs:domain guidance:ConformanceLevel ;
    rdfs:range xsd:string .

guidance:property_conformanceLevel sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:conformanceLevel .

guidance:property_hasComplianceMetrics sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:hasComplianceMetrics .

guidance:property_hasIntegrationStep sh:class guidance:IntegrationStep ;
    sh:minCount 1 ;
    sh:path guidance:hasIntegrationStep .

guidance:property_hasMinimumRequirements sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:hasMinimumRequirements .

guidance:property_hasStringRepresentation sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:hasStringRepresentation .

guidance:property_hasTestPhase sh:class guidance:TestPhase ;
    sh:maxCount 5 ;
    sh:minCount 1 ;
    sh:path guidance:hasTestPhase .

guidance:property_hasValidationRules sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:hasValidationRules .

guidance:property_requiresNamespaceValidation sh:class xsd:boolean ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:requiresNamespaceValidation .

guidance:property_requiresPrefixValidation sh:class xsd:boolean ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:requiresPrefixValidation .

guidance:requiresNamespaceValidation a owl:ObjectProperty ;
    rdfs:label "requires namespace validation"@en ;
    rdfs:comment "Whether namespace validation is required"@en ;
    rdfs:domain guidance:TestProtocol ;
    rdfs:range xsd:boolean .

guidance:requiresPrefixValidation a owl:ObjectProperty ;
    rdfs:label "requires prefix validation"@en ;
    rdfs:comment "Whether prefix validation is required"@en ;
    rdfs:domain guidance:TestProtocol ;
    rdfs:range xsd:boolean .

guidance:stepDescription a owl:ObjectProperty ;
    rdfs:label "step description"@en ;
    rdfs:comment "Description of the integration step"@en ;
    rdfs:domain guidance:IntegrationStep ;
    rdfs:range xsd:string .

guidance:stepOrder a owl:ObjectProperty ;
    rdfs:label "step order"@en ;
    rdfs:comment "The order of the integration step"@en ;
    rdfs:domain guidance:IntegrationStep ;
    rdfs:range xsd:string .

guidance:uri a owl:Ontology ;
    rdfs:label "Guidance Ontology"@en ;
    rdfs:comment "Core guidance ontology for the ontology framework"@en ;
    owl:versionInfo "0.1.0" .

guidance:IntegrationProcess a owl:Class ;
    rdfs:label "Integration Process"@en ;
    rdfs:comment "Process for integrating model changes"@en .

guidance:ModelConformance a owl:Class ;
    rdfs:label "Model Conformance"@en ;
    rdfs:comment "Rules for ensuring model consistency"@en ;
    rdfs:subClassOf guidance:ValidationPattern .

guidance:TODO a owl:Class ;
    rdfs:label "Future Improvements",
        "Future Improvements"@en ;
    rdfs:comment "Items for future development and enhancement",
        "Items for future development and enhancement"@en .

guidance:property_stepDescription sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:stepDescription .

guidance:property_stepOrder sh:class xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path guidance:stepOrder .

guidance:TestProtocol a owl:Class ;
    rdfs:label "Test Protocol"@en ;
    rdfs:comment "A protocol for testing ontology components"@en .

guidance:IntegrationStep a owl:Class ;
    rdfs:label "Integration Step"@en ;
    rdfs:comment "A step in the integration process"@en ;
    rdfs:subClassOf guidance:ValidationPattern .

guidance:TestPhase a owl:Class ;
    rdfs:label "Test Phase"@en ;
    rdfs:comment "A phase in the testing process"@en ;
    rdfs:subClassOf guidance:ValidationPattern .

guidance:ConformanceLevel a owl:Class ;
    rdfs:label "Conformance Level"@en ;
    rdfs:comment "A level of conformance for validation"@en .

guidance:ValidationPattern a owl:Class ;
    rdfs:label "Validation Pattern"@en ;
    rdfs:comment "Pattern for implementing validation rules"@en .

