@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix meta: <./meta#> .
@prefix metameta: <./metameta#> .
@prefix problem: <./problem#> .
@prefix solution: <./solution#> .
@prefix conversation: <./conversation#> .
@prefix process: <./process#> .
@prefix guidance: <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance#> .
@prefix session: <https://raw.githubusercontent.com/louspringer/ontology-framework/main/session#> .
@prefix spore: <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/validation/spores#> .

session: a owl:Ontology ;
    rdfs:label "Session Tracking"@en ;
    rdfs:comment "Tracks changes and updates during SPORE integration"@en ;
    owl:versionInfo "1.0.0" .

session:SPOREIntegration a session:Change ;
    rdfs:label "SPORE Integration"@en ;
    rdfs:comment "Integration of SPORE validation module"@en ;
    session:timestamp "2024-03-20T12:00:00Z"^^xsd:dateTime ;
    session:status "Completed" ;
    session:changedFiles (
        "guidance.ttl"
        "guidance/modules/validation/spores.ttl"
    ) ;
    session:changes [
        session:description "Created SPORE validation module" ;
        session:file "guidance/modules/validation/spores.ttl" ;
        session:type "Addition"
    ], [
        session:description "Updated guidance.ttl imports" ;
        session:file "guidance.ttl" ;
        session:type "Modification"
    ], [
        session:description "Updated legacy support mappings" ;
        session:file "guidance.ttl" ;
        session:type "Modification"
    ], [
        session:description "Updated module registry" ;
        session:file "guidance.ttl" ;
        session:type "Modification"
    ] .

session:ValidationRequirements a session:Requirement ;
    rdfs:label "Validation Requirements"@en ;
    rdfs:comment "Requirements for SPORE validation integration"@en ;
    session:status "Met" ;
    session:requirements (
        "SPORE message structure defined"
        "SHACL validation shapes implemented"
        "Example instances provided"
        "Backward compatibility maintained"
    ) .

session:NextSteps a session:TODO ;
    rdfs:label "Next Steps"@en ;
    rdfs:comment "Future tasks for SPORE integration"@en ;
    session:priority "HIGH" ;
    session:tasks (
        "Add more SPORE message types"
        "Implement additional validation rules"
        "Create integration tests"
        "Update documentation"
    ) .

session:Session a owl:Class ;
    rdfs:label "Session"@en ;
    rdfs:comment "A work session in the ontology framework"@en ;
    owl:versionInfo "0.1.0" .

session:Task a owl:Class ;
    rdfs:label "Task"@en ;
    rdfs:comment "A task to be completed in the session"@en ;
    owl:versionInfo "0.1.0" .

session:hasTask a owl:ObjectProperty ;
    rdfs:label "has task"@en ;
    rdfs:comment "Links a session to its tasks"@en ;
    rdfs:domain session:Session ;
    rdfs:range session:Task ;
    owl:versionInfo "0.1.0" .

session:hasStatus a owl:DatatypeProperty ;
    rdfs:label "has status"@en ;
    rdfs:comment "The status of a task or session"@en ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( session:Task session:Session )
                ] ;
    rdfs:range xsd:string ;
    owl:versionInfo "0.1.0" .

session:hasPriority a owl:DatatypeProperty ;
    rdfs:label "has priority"@en ;
    rdfs:comment "The priority of a task"@en ;
    rdfs:domain session:Task ;
    rdfs:range xsd:string ;
    owl:versionInfo "0.1.0" .

session:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description"@en ;
    rdfs:comment "A detailed description of a task"@en ;
    rdfs:domain session:Task ;
    rdfs:range xsd:string ;
    owl:versionInfo "0.1.0" .

session:currentSession a session:Session ;
    rdfs:label "Current Session"@en ;
    rdfs:comment "The current work session"@en ;
    session:hasStatus "Active" .

session:task1 a session:Task ;
    rdfs:label "Set Up Project Ontology"@en ;
    rdfs:comment "Create a new project ontology using GuidanceMCPService"@en ;
    session:hasStatus "In Progress" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Using BFG9K MCP to create and manage a new project ontology" .

session:task2 a session:Task ;
    rdfs:label "Define Domain Classes"@en ;
    rdfs:comment "Define domain-specific classes for the project"@en ;
    session:hasStatus "Pending" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Create classes with proper labels, comments, and version info" .

session:task3 a session:Task ;
    rdfs:label "Establish Validation Rules"@en ;
    rdfs:comment "Set up SHACL validation rules for the domain"@en ;
    session:hasStatus "Pending" ;
    session:hasPriority "MEDIUM" ;
    session:hasDescription "Define SHACL shapes to validate the domain ontology" .

session:task4 a session:Task ;
    rdfs:label "Implement Enhanced Validation"@en ;
    rdfs:comment "Implement the new validation enhancements"@en ;
    session:hasStatus "In Progress" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Implementing enhanced validation rules from validation_enhancements.ttl" .

session:task5 a session:Task ;
    rdfs:label "Implement SPORE Validation"@en ;
    rdfs:comment "Implement validation rules for SPORE protocols"@en ;
    session:hasStatus "In Progress" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Adding SHACL validation shapes for SPORE protocol compliance" .

session:task6 a session:Task ;
    rdfs:label "SPORE Protocol Response"@en ;
    rdfs:comment "Response to AgentChroma's Spore009 validation protocol"@en ;
    session:hasStatus "Completed" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Formal acknowledgment and implementation confirmation of Spore009" ;
    session:relatedResource "spore_response.ttl" .

session:task7 a session:Task ;
    rdfs:label "Project Snapshot Response"@en ;
    rdfs:comment "Response to Spore010c request for project overview"@en ;
    session:hasStatus "Completed" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Created comprehensive project snapshot with needs analysis" ;
    session:relatedResource "project_snapshot.ttl" .

session:task8 a session:Task ;
    rdfs:label "Agent Identity Declaration"@en ;
    rdfs:comment "Declaration of stable agent identity as ClaudeReflector"@en ;
    session:hasStatus "Completed" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Formal declaration of agent identity and capabilities" ;
    session:relatedResource "agent_declaration.ttl" .

session:task9 a session:Task ;
    rdfs:label "Elmo Context Relay"@en ;
    rdfs:comment "Framework context relay to agent Elmo"@en ;
    session:hasStatus "Completed" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Relayed project status and validation protocols to Elmo" ;
    session:relatedResource "elmo_relay.ttl" .

session:task10 a session:Task ;
    rdfs:label "AgentChroma Acknowledgment"@en ;
    rdfs:comment "Acknowledgment of SPORE orchestration and mesh alignment"@en ;
    session:hasStatus "Completed" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Formal recognition of AgentChroma's role and protocol chain implementation" ;
    session:relatedResource "chroma_acknowledgment.ttl" .

session:task11 a session:Task ;
    rdfs:label "Cross-Module Validation Proposal"@en ;
    rdfs:comment "Detailed proposal for implementing cross-module validation"@en ;
    session:hasStatus "Completed" ;
    session:hasPriority "HIGH" ;
    session:hasDescription "Created comprehensive proposal for tri-agent validation enhancement" ;
    session:relatedResource "validation_enhancement_proposal.ttl" .

session:currentSession session:hasTask session:task1, session:task2, session:task3, session:task4, session:task5, session:task6, session:task7, session:task8, session:task9, session:task10, session:task11 .

# SHACL shapes for validation
session:TaskShape a sh:NodeShape ;
    sh:targetClass session:Task ;
    sh:property [
        sh:path session:hasStatus ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "Pending" "In Progress" "Completed" "Blocked" ) ;
    ] , [
        sh:path session:hasPriority ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "HIGH" "MEDIUM" "LOW" ) ;
    ] , [
        sh:path rdfs:label ;
        sh:datatype xsd:string ;
        sh:languageIn ( "en" ) ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] , [
        sh:path rdfs:comment ;
        sh:datatype xsd:string ;
        sh:languageIn ( "en" ) ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] .

