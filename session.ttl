@prefix : <./session#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix guidance: <./guidance#> .
@prefix collab: <./guidance/modules/collaboration#> .

:SessionOntology rdf:type owl:Ontology ;
    rdfs:label "Session Tracking Ontology" ;
    rdfs:comment "Tracks tasks and changes during development sessions" ;
    owl:versionInfo "1.0.0" ;
    dct:created "2024-03-06T00:25:00Z"^^xsd:dateTime .

:Task rdf:type owl:Class ;
    rdfs:label "Task" ;
    rdfs:comment "A development or configuration task" .

:status rdf:type owl:DatatypeProperty ;
    rdfs:label "Status" ;
    rdfs:comment "Current status of a task" ;
    rdfs:domain :Task ;
    rdfs:range xsd:string .

:description rdf:type owl:DatatypeProperty ;
    rdfs:label "Description" ;
    rdfs:comment "Detailed description of a task" ;
    rdfs:domain :Task ;
    rdfs:range xsd:string .

:GitHubIntegrationTask rdf:type :Task ;
    rdfs:label "GitHub Integration" ;
    dct:created "2024-03-06T00:25:00Z"^^xsd:dateTime ;
    :status "Completed" ;
    :description """Set up GitHub configuration source provider for OCI Resource Manager with the following:
    1. Created GitHub access token configuration
    2. Configured provider in OCI Resource Manager
    3. Verified provider status is ACTIVE
    4. Created feature branch following naming convention
    5. Updated session tracking""" ;
    collab:followsBranchStrategy [
        rdf:type collab:BranchStrategy ;
        collab:branchNameFormat "feature/issue-1-github-integration"
    ] . 