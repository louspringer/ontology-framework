@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

# Classes - proper class definitions
:Person rdf:type owl:Class .
:Position rdf:type owl:Class .
:Company rdf:type owl:Class .
:Interview rdf:type owl:Class .
:OntologyModel rdf:type owl:Class .

# Properties need to be defined before use
:isCandidateFor rdf:type owl:ObjectProperty .
:skills rdf:type owl:DatatypeProperty .
:experience rdf:type owl:ObjectProperty .
:role rdf:type owl:DatatypeProperty .
:company rdf:type owl:DatatypeProperty .
:duration rdf:type owl:DatatypeProperty .
:seeksRoleAttributes rdf:type owl:DatatypeProperty .

# Add explicit property definitions
:reportsTo rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Person ;
    rdfs:label "reports to" ;
    rdfs:comment "Indicates reporting relationship between people" .

# Add inverse properties
:oversees owl:inverseOf :isOverseen .

# Add validation for edge connections
:Edge rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :connectsNode ;
    owl:cardinality "2"^^xsd:nonNegativeInteger
] .

