@prefix rdf: <http:/www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http:/www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http:/www.w3.org/2002/07/owl#> .
@prefix xsd: <http:/www.w3.org/2001/XMLSchema#> .
@prefix test: <http:/example.org/test#> .
@prefix error: <http:/example.org/error#> .

# Test Classes
test:TestClass a owl:Class ;
    rdfs:label "Test Class" ;
    rdfs:comment "A test class for validation" ;
    owl:versionInfo "1.0.0" .

test:SecurityConcept a owl:Class ;
    rdfs:label "Security Concept" ;
    rdfs:comment "A security concept for testing" ;
    owl:versionInfo "1.0.0" .

test:Component a owl:Class ;
    rdfs:label "Component" ;
    rdfs:comment "A system component" ;
    owl:versionInfo "1.0.0" .

# Test Properties
test:hasSecurity a owl:ObjectProperty ;
    rdfs:domain test:Component ;
    rdfs:range test:SecurityConcept ;
    rdfs:label "has security" ;
    rdfs:comment "Links a component to its security concepts" .

test:dependsOn a owl:ObjectProperty ;
    rdfs:domain test:Component ;
    rdfs:range test:Component ;
    rdfs:label "depends on" ;
    rdfs:comment "Indicates component dependencies" .

# Test Individuals
test:Component1 a test:Component ;
    rdfs:label "Component 1" ;
    test:hasSecurity test:SecurityConcept1 ;
    test:dependsOn test:Component2 .

test:Component2 a test:Component ;
    rdfs:label "Component 2" ;
    test:hasSecurity test:SecurityConcept2 .

test:SecurityConcept1 a test:SecurityConcept ;
    rdfs:label "Security Concept 1" .

test:SecurityConcept2 a test:SecurityConcept ;
    rdfs:label "Security Concept 2" .

# RCA Issues
test:RCAIssue a owl:Class ;
    rdfs:label "RCA Issue" ;
    rdfs:comment "Root Cause Analysis Issue" .

test:hasRootCause a owl:ObjectProperty ;
    rdfs:domain test:RCAIssue ;
    rdfs:range test:Component ;
    rdfs:label "has root cause" .

test:Issue1 a test:RCAIssue ;
    rdfs:label "Issue 1" ;
    test:hasRootCause test:Component1 . 