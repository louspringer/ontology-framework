@prefix : <https://raw.githubusercontent.com/louspringer/ontology-framework/main/tests/#> .
@prefix test: <http://example.org/test#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Test Suites
:TestGraphDB a test:TestSuite ;
    rdfs:label "GraphDB Test Suite" ;
    rdfs:comment "Tests for GraphDB functionality" .

# Test Cases
:test_graphdb_connection a test:Test ;
    rdfs:label "test_graphdb_connection" ;
    rdfs:comment "Test GraphDB connection" .

:test_graphdb_query a test:Test ;
    rdfs:label "test_graphdb_query" ;
    rdfs:comment "Test GraphDB query execution" .

:test_graphdb_update a test:Test ;
    rdfs:label "test_graphdb_update" ;
    rdfs:comment "Test GraphDB update operations" .

# Test Dependencies
:test_graphdb_connection test:dependsOn :test_graphdb_query .
:test_graphdb_query test:dependsOn :test_graphdb_update .

