@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Shape for ValidationRuleType class
:ValidationRuleTypeShape a sh:NodeShape ;
    sh:targetClass :ValidationRuleType ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype rdf:langString ;
        sh:languageIn ("en")
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype rdf:langString ;
        sh:languageIn ("en")
    ] ;
    sh:property [
        sh:path :priority ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:integer
    ] ;
    sh:property [
        sh:path :description ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] .

# Shape for ValidationRuleType instances
:ValidationRuleTypeInstanceShape a sh:NodeShape ;
    sh:targetClass :ValidationRuleType ;
    sh:in (
        :RISK
        :MATRIX
        :SEMANTIC
        :SYNTAX
        :SPORE
    ) .

