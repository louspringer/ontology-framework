{
  "mcpServers": {
    "datapilot": {}
  },
  "validation": {
    "enabled": true,
    "strict": false,
    "rules": {
      "phaseOrder": true,
      "contextRequired": true,
      "serverConfigRequired": true
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "mcp.log"
  }
}