{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_baldo_devbox_fr_name": {
            "defaultValue": "baldo-devbox-fr",
            "type": "String"
        },
        "disks_baldo_devbox_fr_OsDisk_1_4e84a2bba3844211a587ef4eb877dc76_externalid": {
            "defaultValue": "/subscriptions/dae16755-7832-4e6d-bae2-c49ff833042f/resourceGroups/baldo-devbox-rg/providers/Microsoft.Compute/disks/baldo-devbox-fr_OsDisk_1_4e84a2bba3844211a587ef4eb877dc76",
            "type": "String"
        },
        "networkInterfaces_baldo_devbox_fr896_externalid": {
            "defaultValue": "/subscriptions/dae16755-7832-4e6d-bae2-c49ff833042f/resourceGroups/baldo-devbox-rg/providers/Microsoft.Network/networkInterfaces/baldo-devbox-fr896",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2024-11-01",
            "name": "[parameters('virtualMachines_baldo_devbox_fr_name')]",
            "location": "northeurope",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_D4s_v3"
                },
                "additionalCapabilities": {
                    "hibernationEnabled": false
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "canonical",
                        "offer": "0001-com-ubuntu-server-jammy",
                        "sku": "22_04-lts-gen2",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
