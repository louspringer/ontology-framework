@base <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/error_testing#> .

@prefix : <./#> .
@prefix ns1: <./runtime_error_handling#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:exampleTestAssumption1 a ns1:TestAssumption  ;
    rdfs:label "Example TestAssumption 1" ;
    rdfs:comment "This is example instance 1 of class TestAssumption" .

:exampleTestAssumption2 a ns1:TestAssumption  ;
    rdfs:label "Example TestAssumption 2" ;
    rdfs:comment "This is example instance 2 of class TestAssumption" .

:exampleTestCase1 a ns1:TestCase  ;
    rdfs:label "Example TestCase 1" ;
    rdfs:comment "This is example instance 1 of class TestCase" ;
    ns1:hasTestImplementation :exampleTestImplementation1 ;
    ns1:validatesRequirement :exampleRequirement1 .

:exampleTestCase2 a ns1:TestCase  ;
    rdfs:label "Example TestCase 2" ;
    rdfs:comment "This is example instance 2 of class TestCase" ;
    ns1:hasTestImplementation :exampleTestImplementation2 ;
    ns1:validatesRequirement :exampleRequirement2 .

:exampleTestStep1 a ns1:TestStep  ;
    rdfs:label "Example TestStep 1" ;
    rdfs:comment "This is example instance 1 of class TestStep" .

:exampleTestStep2 a ns1:TestStep  ;
    rdfs:label "Example TestStep 2" ;
    rdfs:comment "This is example instance 2 of class TestStep" .

ns1:ErrorTypeExclusivity a ns1:TestAssumption ;
    rdfs:label "Error Type Exclusivity" ;
    rdfs:comment "Each error belongs to exactly one type" .

ns1:ErrorTypeTestCase a ns1:TestCase ;
    rdfs:label "Error Type Test Case" ;
    rdfs:comment "Test case for error type handling" ;
    ns1:hasTestImplementation ns1:ErrorTypeTestImplementation ;
    ns1:validatesRequirement ns1:ErrorTypeRequirement .

ns1:TestErrorAnalysis a ns1:TestStep ;
    rdfs:label "Test Error Analysis" ;
    rdfs:comment "Step to analyze test errors" ;
    ns1:hasStepOrder 2 .

ns1:TestErrorIdentification a ns1:TestStep ;
    rdfs:label "Test Error Identification" ;
    rdfs:comment "Step to identify test errors" ;
    ns1:hasStepOrder 1 .

ns1:TestErrorResolution a ns1:TestStep ;
    rdfs:label "Test Error Resolution" ;
    rdfs:comment "Step to resolve test errors" ;
    ns1:hasStepOrder 3 .

ns1:TestErrorVerification a ns1:TestStep ;
    rdfs:label "Test Error Verification" ;
    rdfs:comment "Step to verify test error resolution" ;
    ns1:hasStepOrder 4 .

ns1:hasTestImplementation a owl:ObjectProperty ;
    rdfs:domain ns1:TestCase ;
    rdfs:range ns1:TestImplementation .

ns1:validatesRequirement a owl:ObjectProperty ;
    rdfs:domain ns1:TestCase ;
    rdfs:range ns1:Requirement .

:exampleRequirement1 a ns1:Requirement  ;
    rdfs:label "Example Requirement 1" ;
    rdfs:comment "This is example instance 1 of class Requirement" .

:exampleRequirement2 a ns1:Requirement  ;
    rdfs:label "Example Requirement 2" ;
    rdfs:comment "This is example instance 2 of class Requirement" .

:exampleTestImplementation1 a ns1:TestImplementation  ;
    rdfs:label "Example TestImplementation 1" ;
    rdfs:comment "This is example instance 1 of class TestImplementation" .

:exampleTestImplementation2 a ns1:TestImplementation  ;
    rdfs:label "Example TestImplementation 2" ;
    rdfs:comment "This is example instance 2 of class TestImplementation" .

ns1:ErrorTypeRequirement a ns1:Requirement ;
    rdfs:label "Error Type Requirement" ;
    rdfs:comment "Requirement for error type handling" .

ns1:ErrorTypeTestImplementation a ns1:TestImplementation ;
    rdfs:label "Error Type Test Implementation" ;
    rdfs:comment "Implementation of error type test case" .

ns1:TestAssumption a owl:Class ;
    rdfs:label "Test Assumption" ;
    rdfs:comment "Assumptions for error handling tests" .

ns1:Requirement a owl:Class ;
    rdfs:label "Requirement" ;
    rdfs:comment "Error handling requirements" .

ns1:TestImplementation a owl:Class ;
    rdfs:label "Test Implementation" ;
    rdfs:comment "Implementation of test cases" .

ns1:TestCase a owl:Class ;
    rdfs:label "Test Case" ;
    rdfs:comment "Test cases for error handling" .

ns1:TestStep a owl:Class ;
    rdfs:label "Test Step" ;
    rdfs:comment "Steps in test execution" .

[] a sh:NodeShape ;
    sh:property :shape1sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path rdfs:comment  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path rdfs:label  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path owl:versionInfo ;
    sh:targetClass owl:Class .

[] a sh:NodeShape ;
    sh:property :shape2sh:minCount 1 ;
            sh:path rdfs:range  .
        :shape1 sh:minCount 1  ;
            sh:path rdfs:domain ;
    sh:targetClass rdf:Property .

