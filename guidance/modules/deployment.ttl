@base <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/deployment#> .

@prefix : <./#> .
@prefix ns1: <http://example.org/guidance#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:colimaRuntime a ns1:ColimaRuntime ;
    rdfs:label "Colima Runtime" ;
    ns1:hasRuntimeName "colima" ;
    ns1:hasRuntimeStatus "running" ;
    ns1:hasRuntimeVersion "0.5.6" ;
    rdfs:comment "Colima container runtime configuration" .

ns1:datasetCheckTest a ns1:JenaFusekiTest ;
    rdfs:label "Dataset Check Test" ;
    ns1:hasExpectedResponse "200" ;
    ns1:hasTestEndpoint "http://localhost:3030/guidance" ;
    ns1:hasTestName "dataset_check" ;
    rdfs:comment "Test for dataset existence" .

ns1:errorLogTest a ns1:LoggingTest ;
    rdfs:label "Error Log Test" ;
    ns1:hasLogLevel "ERROR" ;
    ns1:hasLogPattern "" ;
    ns1:hasTestName "error_log_check" ;
    rdfs:comment "Test for error log patterns" .

ns1:fusekiDeployment a ns1:JenaFusekiDeployment ;
    rdfs:label "Fuseki Deployment" ;
    ns1:hasDatasetName "guidance" ;
    ns1:hasServiceEndpoint "http://localhost:3030" ;
    ns1:hasServiceName "fuseki" ;
    ns1:hasServicePort 3030 ;
    ns1:hasServiceVersion "4.10.0" ;
    rdfs:comment "Deployment configuration for Jena Fuseki" .

ns1:hasDatasetName a owl:DatatypeProperty ;
    rdfs:label "Dataset Name" ;
    rdfs:comment "Name of the dataset" ;
    rdfs:domain ns1:JenaFusekiDeployment ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasExpectedResponse a owl:DatatypeProperty ;
    rdfs:label "Expected Response" ;
    rdfs:comment "Expected response from the test" ;
    rdfs:domain ns1:JenaFusekiTest ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasLogLevel a owl:DatatypeProperty ;
    rdfs:label "Log Level" ;
    rdfs:comment "Log level to check" ;
    rdfs:domain ns1:LoggingTest ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasLogPattern a owl:DatatypeProperty ;
    rdfs:label "Log Pattern" ;
    rdfs:comment "Pattern to match in logs" ;
    rdfs:domain ns1:LoggingTest ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasRuntimeName a owl:DatatypeProperty ;
    rdfs:label "Runtime Name" ;
    rdfs:comment "Name of the container runtime" ;
    rdfs:domain ns1:ContainerRuntime ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasRuntimeStatus a owl:DatatypeProperty ;
    rdfs:label "Runtime Status" ;
    rdfs:comment "Current status of the container runtime" ;
    rdfs:domain ns1:ContainerRuntime ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasRuntimeVersion a owl:DatatypeProperty ;
    rdfs:label "Runtime Version" ;
    rdfs:comment "Version of the container runtime" ;
    rdfs:domain ns1:ContainerRuntime ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasServiceEndpoint a owl:DatatypeProperty ;
    rdfs:label "Service Endpoint" ;
    rdfs:comment "Base URL for the service" ;
    rdfs:domain ns1:ServiceDeployment ;
    rdfs:range xsd:anyURI ;
    owl:versionInfo "1.0.0" .

ns1:hasServiceName a owl:DatatypeProperty ;
    rdfs:label "Service Name" ;
    rdfs:comment "Name of the service" ;
    rdfs:domain ns1:ServiceDeployment ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasServicePort a owl:DatatypeProperty ;
    rdfs:label "Service Port" ;
    rdfs:comment "Port number for the service" ;
    rdfs:domain ns1:ServiceDeployment ;
    rdfs:range xsd:integer ;
    owl:versionInfo "1.0.0" .

ns1:hasServiceVersion a owl:DatatypeProperty ;
    rdfs:label "Service Version" ;
    rdfs:comment "Version of the service" ;
    rdfs:domain ns1:ServiceDeployment ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:hasTestEndpoint a owl:DatatypeProperty ;
    rdfs:label "Test Endpoint" ;
    rdfs:comment "Endpoint to test" ;
    rdfs:domain ns1:JenaFusekiTest ;
    rdfs:range xsd:anyURI ;
    owl:versionInfo "1.0.0" .

ns1:hasTestName a owl:DatatypeProperty ;
    rdfs:label "Test Name" ;
    rdfs:comment "Name of the test" ;
    rdfs:domain ns1:TestConfiguration ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

ns1:healthCheckTest a ns1:JenaFusekiTest ;
    rdfs:label "Health Check Test" ;
    ns1:hasExpectedResponse "200" ;
    ns1:hasTestEndpoint "http://localhost:3030/" ;
    ns1:hasTestName "health_check" ;
    rdfs:comment "Test for Fuseki health check endpoint" .

ns1:serverLogTest a ns1:LoggingTest ;
    rdfs:label "Server Log Test" ;
    ns1:hasLogLevel "INFO" ;
    ns1:hasLogPattern "Server started" ;
    ns1:hasTestName "log_check" ;
    rdfs:comment "Test for server log patterns" .

