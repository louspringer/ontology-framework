ns1:ColimaRuntime a owl:Class ;
    rdfs:label "Colima Runtime" ;
    rdfs:comment "Configuration for Colima container runtime" ;
    rdfs:subClassOf ns1:ContainerRuntime ;
    owl:versionInfo "1.0.0" .

ns1:Deployment a owl:Class ;
    rdfs:label "Deployment" ;
    rdfs:comment "Base class for deployment configurations" ;
    owl:versionInfo "1.0.0" .

ns1:JenaFusekiDeployment a owl:Class ;
    rdfs:label "Jena Fuseki Deployment" ;
    rdfs:comment "Configuration for Jena Fuseki deployment" ;
    rdfs:subClassOf ns1:ServiceDeployment ;
    owl:versionInfo "1.0.0" .

ns1:TestConfiguration a owl:Class ;
    rdfs:label "Test Configuration" ;
    rdfs:comment "Base class for test configurations" ;
    owl:versionInfo "1.0.0" .

ns1:ContainerRuntime a owl:Class ;
    rdfs:label "Container Runtime" ;
    rdfs:comment "Configuration for container runtime environment" ;
    rdfs:subClassOf ns1:Deployment ;
    owl:versionInfo "1.0.0" .

ns1:JenaFusekiTest a owl:Class ;
    rdfs:label "Jena Fuseki Test" ;
    rdfs:comment "Configuration for Jena Fuseki tests" ;
    rdfs:subClassOf ns1:TestConfiguration ;
    owl:versionInfo "1.0.0" .

ns1:LoggingTest a owl:Class ;
    rdfs:label "Logging Test" ;
    rdfs:comment "Configuration for logging tests" ;
    rdfs:subClassOf ns1:TestConfiguration ;
    owl:versionInfo "1.0.0" .

ns1:ServiceDeployment a owl:Class ;
    rdfs:label "Service Deployment" ;
    rdfs:comment "Configuration for service deployment" ;
    rdfs:subClassOf ns1:Deployment ;
    owl:versionInfo "1.0.0" .

[] a sh:NodeShape ;
    sh:property :shape1sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path rdfs:comment  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path owl:versionInfo  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path rdfs:label ;
    sh:targetClass owl:Class .

[] a sh:NodeShape ;
    sh:property :shape2sh:minCount 1 ;
            sh:path rdfs:domain  .
        :shape1 sh:minCount 1  ;
            sh:path rdfs:range ;
    sh:targetClass rdf:Property .

