    <rdfs:label>has source code</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/provenance#ModelGenerator"/>
    <rdfs:range rdf:resource="http://example.org/code#CodeArtifact"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/provenance#hasDependencies">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has dependencies</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/provenance#ModelGenerator"/>
    <rdfs:range rdf:resource="http://example.org/code#CodeArtifact"/>
  </rdf:Description>
</rdf:RDF>
