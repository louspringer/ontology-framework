step:ErrorAnalysisInstance a step:ErrorAnalysis ;
    step:hasStepAction "Analyze error cause and impact" ;
    step:hasStepOrder 2 .

step:ErrorHandlingStepShape a sh:NodeShape ;
    sh:property :shape1sh:datatype xsd:integer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path step:hasStepOrder  .
        :shape1 sh:datatype xsd:string  ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path step:hasStepAction ;
    sh:targetClass step:ErrorHandlingStep .

step:ErrorIdentificationInstance a step:ErrorIdentification ;
    step:hasStepAction "Identify error type and severity" ;
    step:hasStepOrder 1 .

step:ErrorPreventionInstance a step:ErrorPrevention ;
    step:hasStepAction "Prevent future errors" ;
    step:hasStepOrder 4 .

step:ErrorRecoveryInstance a step:ErrorRecovery ;
    step:hasStepAction "Recover from the error" ;
    step:hasStepOrder 3 .

step:TestErrorAnalysisInstance a step:TestErrorAnalysis ;
    step:hasStepAction "Analyze test errors" ;
    step:hasStepOrder 2 .

step:TestErrorIdentificationInstance a step:TestErrorIdentification ;
    step:hasStepAction "Identify test errors" ;
    step:hasStepOrder 1 .

step:TestErrorPreventionInstance a step:TestErrorPrevention ;
    step:hasStepAction "Prevent future test errors" ;
    step:hasStepOrder 4 .

step:TestErrorRecoveryInstance a step:TestErrorRecovery ;
    step:hasStepAction "Recover from test errors" ;
    step:hasStepOrder 3 .

step:hasStepAction a owl:DatatypeProperty ;
    rdfs:label "Has Step Action" ;
    rdfs:domain step:ErrorHandlingStep ;
    rdfs:range xsd:string ;
    owl:versionInfo "1.0.0" .

step:hasStepOrder a owl:DatatypeProperty ;
    rdfs:label "Has Step Order" ;
    rdfs:domain step:ErrorHandlingStep ;
    rdfs:range xsd:integer ;
    owl:versionInfo "1.0.0" .

step:ErrorAnalysis a owl:Class ;
    rdfs:label "Error Analysis" ;
    rdfs:comment "Step to analyze error causes and impacts" ;
    rdfs:subClassOf step:ErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:ErrorIdentification a owl:Class ;
    rdfs:label "Error Identification" ;
    rdfs:comment "Step to identify and classify errors" ;
    rdfs:subClassOf step:ErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:ErrorPrevention a owl:Class ;
    rdfs:label "Error Prevention" ;
    rdfs:comment "Step to prevent future errors" ;
    rdfs:subClassOf step:ErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:ErrorRecovery a owl:Class ;
    rdfs:label "Error Recovery" ;
    rdfs:comment "Step to recover from errors" ;
    rdfs:subClassOf step:ErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:TestErrorAnalysis a owl:Class ;
    rdfs:label "Test Error Analysis" ;
    rdfs:comment "Step to analyze test failures" ;
    rdfs:subClassOf step:TestErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:TestErrorIdentification a owl:Class ;
    rdfs:label "Test Error Identification" ;
    rdfs:comment "Step to identify test failures" ;
    rdfs:subClassOf step:TestErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:TestErrorPrevention a owl:Class ;
    rdfs:label "Test Error Prevention" ;
    rdfs:comment "Step to prevent future test failures" ;
    rdfs:subClassOf step:TestErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:TestErrorRecovery a owl:Class ;
    rdfs:label "Test Error Recovery" ;
    rdfs:comment "Step to recover from test failures" ;
    rdfs:subClassOf step:TestErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:TestErrorHandlingStep a owl:Class ;
    rdfs:label "Test Error Handling Step" ;
    rdfs:comment "A step in the test error handling process" ;
    rdfs:subClassOf step:ErrorHandlingStep ;
    owl:versionInfo "1.0.0" .

step:ErrorHandlingStep a owl:Class ;
    rdfs:label "Error Handling Step" ;
    rdfs:comment "A step in the error handling process" ;
    owl:versionInfo "1.0.0" .

[] a sh:NodeShape ;
    sh:property :shape2sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path owl:versionInfo  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path rdfs:label  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path rdfs:comment ;
    sh:targetClass owl:Class .

[] a sh:NodeShape ;
    sh:property :shape3sh:minCount 1 ;
            sh:path rdfs:range  .
        :shape1 sh:minCount 1  ;
            sh:path rdfs:domain ;
    sh:targetClass rdf:Property .

