@base <https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/error_process#> .

@prefix : <./#> .
@prefix ns1: <./runtime_error_handling#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:exampleErrorHandlingStep1 a ns1:ErrorHandlingStep  ;
    rdfs:label "Example ErrorHandlingStep 1" ;
    rdfs:comment "This is example instance 1 of class ErrorHandlingStep" .

:exampleErrorHandlingStep2 a ns1:ErrorHandlingStep  ;
    rdfs:label "Example ErrorHandlingStep 2" ;
    rdfs:comment "This is example instance 2 of class ErrorHandlingStep" .

:examplePreventionMeasure1 a ns1:PreventionMeasure  ;
    rdfs:label "Example PreventionMeasure 1" ;
    rdfs:comment "This is example instance 1 of class PreventionMeasure" .

:examplePreventionMeasure2 a ns1:PreventionMeasure  ;
    rdfs:label "Example PreventionMeasure 2" ;
    rdfs:comment "This is example instance 2 of class PreventionMeasure" .

:exampleRecoveryStrategy1 a ns1:RecoveryStrategy  ;
    rdfs:label "Example RecoveryStrategy 1" ;
    rdfs:comment "This is example instance 1 of class RecoveryStrategy" .

:exampleRecoveryStrategy2 a ns1:RecoveryStrategy  ;
    rdfs:label "Example RecoveryStrategy 2" ;
    rdfs:comment "This is example instance 2 of class RecoveryStrategy" .

ns1:ErrorAnalysis a ns1:ErrorHandlingStep ;
    rdfs:label "Error Analysis" ;
    rdfs:comment "Analysis of error cause and impact" ;
    ns1:hasStepOrder 2 .

ns1:ErrorIdentification a ns1:ErrorHandlingStep ;
    rdfs:label "Error Identification" ;
    rdfs:comment "Initial identification of the error" ;
    ns1:hasStepOrder 1 .

ns1:ErrorResolution a ns1:ErrorHandlingStep ;
    rdfs:label "Error Resolution" ;
    rdfs:comment "Implementation of error fix" ;
    ns1:hasStepOrder 3 .

ns1:ErrorVerification a ns1:ErrorHandlingStep ;
    rdfs:label "Error Verification" ;
    rdfs:comment "Verification of error resolution" ;
    ns1:hasStepOrder 4 .

ns1:Fallback a ns1:RecoveryStrategy ;
    rdfs:label "Fallback" ;
    rdfs:comment "Provides alternative execution path" .

ns1:InputValidation a ns1:PreventionMeasure ;
    rdfs:label "Input Validation" ;
    rdfs:comment "Validates input data before processing" .

ns1:Retry a ns1:RecoveryStrategy ;
    rdfs:label "Retry" ;
    rdfs:comment "Attempts to retry failed operations" .

ns1:TypeChecking a ns1:PreventionMeasure ;
    rdfs:label "Type Checking" ;
    rdfs:comment "Ensures type safety during execution" .

ns1:hasPreventionMeasure a owl:ObjectProperty ;
    rdfs:domain ns1:RuntimeError ;
    rdfs:range ns1:PreventionMeasure .

ns1:hasRecoveryStrategy a owl:ObjectProperty ;
    rdfs:domain ns1:RuntimeError ;
    rdfs:range ns1:RecoveryStrategy .

ns1:PreventionMeasure a owl:Class ;
    rdfs:label "Prevention Measure" ;
    rdfs:comment "Measures to prevent errors" .

ns1:RecoveryStrategy a owl:Class ;
    rdfs:label "Recovery Strategy" ;
    rdfs:comment "Strategies for error recovery" .

ns1:ErrorHandlingStep a owl:Class ;
    rdfs:label "Error Handling Step" ;
    rdfs:comment "A step in the error handling process" .

[] a sh:NodeShape ;
    sh:property :shape1sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path rdfs:label  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path owl:versionInfo  .
        :shape1 sh:datatype xsd:string  ;
            sh:minCount 1 ;
            sh:path rdfs:comment ;
    sh:targetClass owl:Class .

[] a sh:NodeShape ;
    sh:property :shape2sh:minCount 1 ;
            sh:path rdfs:domain  .
        :shape1 sh:minCount 1  ;
            sh:path rdfs:range ;
    sh:targetClass rdf:Property .

