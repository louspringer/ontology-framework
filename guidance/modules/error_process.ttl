@prefix ns1: <./runtime_error_handling#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Error Handling Steps
ns1:ErrorHandlingStep a owl:Class ;
    rdfs:label "Error Handling Step" ;
    rdfs:comment "A step in the error handling process" .

ns1:ErrorIdentification a ns1:ErrorHandlingStep ;
    rdfs:label "Error Identification" ;
    rdfs:comment "Initial identification of the error" ;
    ns1:hasStepOrder 1 .

ns1:ErrorAnalysis a ns1:ErrorHandlingStep ;
    rdfs:label "Error Analysis" ;
    rdfs:comment "Analysis of error cause and impact" ;
    ns1:hasStepOrder 2 .

ns1:ErrorResolution a ns1:ErrorHandlingStep ;
    rdfs:label "Error Resolution" ;
    rdfs:comment "Implementation of error fix" ;
    ns1:hasStepOrder 3 .

ns1:ErrorVerification a ns1:ErrorHandlingStep ;
    rdfs:label "Error Verification" ;
    rdfs:comment "Verification of error resolution" ;
    ns1:hasStepOrder 4 .

# Prevention Measures
ns1:PreventionMeasure a owl:Class ;
    rdfs:label "Prevention Measure" ;
    rdfs:comment "Measures to prevent errors" .

ns1:InputValidation a ns1:PreventionMeasure ;
    rdfs:label "Input Validation" ;
    rdfs:comment "Validates input data before processing" .

ns1:TypeChecking a ns1:PreventionMeasure ;
    rdfs:label "Type Checking" ;
    rdfs:comment "Ensures type safety during execution" .

ns1:hasPreventionMeasure a owl:ObjectProperty ;
    rdfs:domain ns1:RuntimeError ;
    rdfs:range ns1:PreventionMeasure .

# Recovery Strategies
ns1:RecoveryStrategy a owl:Class ;
    rdfs:label "Recovery Strategy" ;
    rdfs:comment "Strategies for error recovery" .

ns1:Retry a ns1:RecoveryStrategy ;
    rdfs:label "Retry" ;
    rdfs:comment "Attempts to retry failed operations" .

ns1:Fallback a ns1:RecoveryStrategy ;
    rdfs:label "Fallback" ;
    rdfs:comment "Provides alternative execution path" .

ns1:hasRecoveryStrategy a owl:ObjectProperty ;
    rdfs:domain ns1:RuntimeError ;
    rdfs:range ns1:RecoveryStrategy . 