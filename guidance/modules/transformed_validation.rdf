  <rdf:Description rdf:about="https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/validation#PolicyStructureTest">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/validation#TestCase"/>
    <rdfs:label>Policy Structure Test</rdfs:label>
    <rdfs:comment>Tests that policies drive requirements that influence decisions</rdfs:comment>
    <ns1:hasExpectedResult>Should find OntologyAccessibilityPolicy and its requirements</ns1:hasExpectedResult>
    <ns1:hasTestQuery>
    SELECT ?policy ?req ?decision WHERE {
        ?policy rdf:type pdi:Policy ;
                pdi:drivesRequirement ?req .
        ?req pdi:influencesDecision ?decision .
    }</ns1:hasTestQuery>
  </rdf:Description>
  <rdf:Description rdf:about="https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/validation#EnforcementMechanismsTest">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://raw.githubusercontent.com/louspringer/ontology-framework/main/guidance/modules/validation#TestCase"/>
    <rdfs:label>Enforcement Mechanisms Test</rdfs:label>
    <rdfs:comment>Tests that enforcement mechanisms are properly defined</rdfs:comment>
    <ns1:hasExpectedResult>Should find at least 3 enforcement mechanisms</ns1:hasExpectedResult>
    <ns1:hasTestQuery>
    SELECT ?mech ?impl ?pattern WHERE {
        ?mech rdf:type pdi:EnforcementMechanism ;
              pdi:implementedIn ?impl ;
              pdi:usesPattern ?pattern .
    }</ns1:hasTestQuery>
  </rdf:Description>
